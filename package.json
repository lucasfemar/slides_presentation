{
  "name": "slides_presentation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && npm run services:wait:database && npm run prisma:push && next dev",
    "test": "npm run services:up && npm run services:wait:database && npm run prisma:push:reset && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "posttest": "npm run services:down",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:push": "dotenv -e .env.development -- npx prisma db push",
    "prisma:push:reset": "dotenv -e .env.development -- npx prisma db push --force-reset",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:wait:database": "node infra/scripts/wait-for-postgres.js"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@prisma/client": "^5.22.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-table": "^8.20.5",
    "@tanstack/react-virtual": "^3.10.8",
    "@types/jsonwebtoken": "^9.0.7",
    "async-retry": "^1.3.3",
    "axios": "^1.7.7",
    "bcrypt": "5.1.1",
    "cookie": "^1.0.2",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "dotenv-expand": "^11.0.6",
    "jsonwebtoken": "^9.0.2",
    "material-react-table": "^3.0.1",
    "next": "^14.2.23",
    "next-connect": "^1.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.3.0",
    "react-toastify": "^10.0.6",
    "styled-components": "^6.1.13",
    "tailwindcss": "^3.4.14",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/bcrypt": "5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
    "concurrently": "^9.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "webpack-cli": "^5.1.4"
  }
}
